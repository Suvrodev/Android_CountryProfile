<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.country_profile">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/bangladesh"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bangladesh"
        android:supportsRtl="true"
        android:theme="@style/Theme.Country_Profile"
        tools:targetApi="31">
        <activity
            android:name=".Activity_Fragment"
            android:screenOrientation="landscape"
            android:exported="false" />
        <activity
            android:name=".Activity_Spinner"
            android:exported="false" />
        <activity
            android:name=".Activity_Recycleview"
            android:exported="false" />
        <activity
            android:name=".Activity_GridView"
            android:exported="false" />
        <activity
            android:name=".Activity_Listview_Custom"
            android:exported="false" />
        <activity
            android:name=".Activity_Listview"
            android:exported="false" />
        <activity
            android:name=".Activity_Profile"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>